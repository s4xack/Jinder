<Page x:Class="Jinder.Desktop.Views.SummariesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Jinder.Desktop.Views"
      xmlns:viewModels="clr-namespace:Jinder.Desktop.ViewModels"
      xmlns:controls="clr-namespace:Jinder.Desktop.Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Requests">
    <Page.DataContext>
        <viewModels:SummariesViewModel />
    </Page.DataContext>
    <controls:SummaryControl Summary="{Binding Summary}" />
</Page>
